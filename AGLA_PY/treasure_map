# 🍓
row1 = ["⬜️","⬜️","⬜️"]
row2 = ["⬜️","⬜️","⬜️"]
row3 = ["⬜️","⬜️","⬜️"]
map = [row1, row2, row3]
print(f"{row1}\n{row2}\n{row3}")
position = input("Where do you want to put the treasure? ")
#column 列
verticl = int(position[1])
#row 行
horizonal = int(position[0])
map[verticl-1][horizonal-1] = '🍓'
print(f"{row1}\n{row2}\n{row3}")

